#include <stdio.h>
#include <stdlib.h>



void heapify(int a[],int r,int b){
	int c = 2*r +1;

	if(c<b)
		c = a[c] > a[c+1] ? c:c+1;
	if(c>b) return;
	if(a[c]<=a[r]) return;

	swap(a[c],a[r]);
	heapify(a,c,b);
}






















int main() {
	int i = 0;
	int arr[6] = { 0, -1, 6, 72, 1, 5 };
	//mergeSort(arr, 0, 5);
	swap(&arr[1],&arr[1]);
	while (i < 6)
		printf("%d ", arr[i++]);
	return 0;
}
